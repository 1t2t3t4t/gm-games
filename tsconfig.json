{
	"compilerOptions": {
		"target": "esnext",
		"resolveJsonModule": true,
		"jsx": "react",
		"rootDir": "./src",
		"noEmit": true,
		"noUnusedLocals": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"player-names": ["src/deion/worker/data/names-test.json"],
			"league-schema": ["build/files/league-schema.json"]
		},
		"esModuleInterop": true,
		"strict": true
	}
}
